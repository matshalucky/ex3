
@{
    ViewBag.Title = "Map";
}



<html>
<head>
    <style>
       
    </style>

</head>
<body>

	@*<img src="~/map.PNG" />*@

	<canvas id="canvas" style=" height:100%; width:100%; position:fixed;
    background:url('../../Content/map.png'); background-size:100% 100%;"></canvas>

	<script>
        
		var c1 = document.getElementById("canvas");
		var context1 = c1.getContext("2d");
		context1.beginPath();
		context1.lineWidth = 1;
		context1.arc(100, 80, 6, 0, 2 * Math.PI);
		context1.fillStyle = 'red';
		context1.fill();
		context1.stroke();
	</script>

</body>
</html>
